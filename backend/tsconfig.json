{
  "compilerOptions": {
      "target": "es6",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "module": "commonjs",
      "rootDir": "./src",
      "removeComments": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "resolveJsonModule": true,
      "skipLibCheck": true
  }
}